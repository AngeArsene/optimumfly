        <tile>
            <square70x70logo src="/assets/favicon.jpg"/>
            <square150x150logo src="/assets/favicon.jpg"/>
            <square310x310logo src="/assets/favicon.jpg"/>
            <wide310x150logo src="/assets/favicon.jpg"/>
